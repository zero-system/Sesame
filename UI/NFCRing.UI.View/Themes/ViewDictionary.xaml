<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:NFCRing.UI.View.Views"
                    xmlns:viewModels="clr-namespace:NFCRing.UI.ViewModel.ViewModels;assembly=NFCRing.UI.ViewModel">
    <DataTemplate DataType="{x:Type viewModels:WizardViewModel}">
        <views:WizardView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:HelloStepViewModel}">
        <views:HelloStepView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:PlaceRingStepViewModel}">
        <views:PlaceRingStepView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:RemoveRingStepViewModel}">
        <views:RemoveRingStepView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:SuccessfullyStepViewModel}">
        <views:SuccessfullyStepView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:LoginStepViewModel}">
        <views:LoginStepView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:FinishedStepViewModel}">
        <views:FinishedStepView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:LoginControlViewModel}">
        <views:LoginControlView/>
    </DataTemplate>
</ResourceDictionary>